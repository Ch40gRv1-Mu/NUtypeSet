uration: 5s;\n    -webkit-animation:fade-in2 5s;\n    width: 300px;\n    height: 35px;\n    margin-top: 30px;\n    margin-left: auto;\n    margin-right: auto;\n}\n.b1{\n    background-color: #1b9ff7;\n    color:white; \n    height: 35px;\n    border: 0;\n    width: 80px;\n    float: right;\n    margin-right: 30px;\n    text-align: center;\n    border-radius: 6px;\n    -webkit-transition-duration: 0.3s; /* Safari */\n    transition-duration: 0.3s;\n}\n.b1:hover{\n    background-color: white;\n    color: #1b9ff7;\n}\n.b3{\n    background-color: #1b9ff7;\n    color:white; \n    height: 35px;\n    border: 0;\n    width: 100px;\n    float: left;\n    margin-left: 30px;\n    text-align: center;\n    border-radius: 6px;\n    -webkit-transition-duration: 0.3s; /* Safari */\n    transition-duration: 0.3s;\n}\n.b3:hover{\n    background-color: white;\n    color: #1b9ff7;\n}\nh1{\n    font-family:  Avenir, Helvetica, Arial, sans-serif;\n    text-align: center;\n    color: black;\n    font-size: 16pt;\n\n}\n</style>"]}]}