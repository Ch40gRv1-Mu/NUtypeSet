;AAIA,QAAA,KAAA,EAAA;AACA,UAAA,WAAA,EAAA,uBADA;AAEA,UAAA,IAAA,EAAA,UAFA;AAGA,UAAA,GAAA,EAAA;AACA,YAAA,IAAA,EAAA;AADA;AAHA,SAJA;AAWA,QAAA,KAAA,EAAA;AACA,UAAA,QAAA,EAAA;AADA,SAXA;AAcA,QAAA,QAAA,EAAA;AACA,UAAA,QAAA,EAAA;AADA;AAdA,OAfA;AALA,KAAA;AAwCA,GA1CA;AA2CA,EAAA,OAAA,EAAA;AACA,IAAA,aADA,yBACA,CADA,EACA,KADA,EACA;AAAA;;AACA,MAAA,CAAA,CAAA,cAAA,GADA,CAEA;;AACA,UAAA,IAAA,GAAA,EAAA,CAHA,CAIA;;AACA,MAAA,IAAA,CAAA,KAAA,GAAA,KAAA,CAAA,KAAA;AACA,MAAA,IAAA,CAAA,QAAA,GAAA,KAAA,CAAA,QAAA,CANA,CAOA;;AACA,MAAA,QAAA,CAAA,IAAA,CAAA,CAAA,IAAA,CACA,UAAA,GAAA,EAAA;AAGA,YAAA,GAAA,CAAA,IAAA,KAAA,CAAA,EAAA;AACA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,GAAA,CAAA,IAAA;;AACA,UAAA,KAAA,CAAA,MAAA,CAAA,QAAA,CAAA,WAAA,EAAA,GAAA,CAAA,IAAA,EAHA,CAKA;;;AACA,UAAA,KAAA,CAAA,OAAA,CAAA,IAAA,CAAA;AAAA,YAAA,IAAA,EAAA;AAAA,WAAA;AAEA,SARA,MAQA;AACA,cAAA,KAAA,GAAA,KAAA,CAAA,YAAA,CAAA;AACA,YAAA,GAAA,EAAA,YADA;AAEA,YAAA,IAAA,EAAA,OAFA;AAGA,YAAA,IAAA,EAAA;AAHA,WAAA,CAAA;;AAKA,UAAA,KAAA,CAAA,IAAA;AACA;AACA,OApBA;AAsBA,KA/BA;AAgCA,IAAA,OAAA,EAAA,mBAAA;AACA,WAAA,OAAA,CAAA,IAAA,CAAA;AAAA,QAAA,IAAA,EAAA;AAAA,OAAA;AAEA;AAnCA;AA3CA,CAAA","sourcesContent":["<template>\n<div id=\"wrap\">\n    <div class=\"nav\">\n      <button class=\"b1\" @click=\"goIndex\">back</button>\n    </div>\n  <div class=\"main\">\n    <cube-form :model=\"model\" @submit=\"submitHandler\">\n      \n      <cube-form-group>\n       \n        <!--email-->\n        <cube-form-item :field=\"fields[0]\"></cube-form-item>\n        <!--密码-->\n        <cube-form-item :field=\"fields[1]\"></cube-form-item>\n      </cube-form-group>\n\n      <cube-form-group>\n        <button class=\"b3\">Login</button>\n      </cube-form-group>\n\n    </cube-form>\n    <router-link to=\"/register\" class=\"reg\">Register</router-link> \n  </div>\n</div>\n</template>\n\n<script>\n//登录接口\nimport { loginApi } from \"@/api/http.js\";\nexport default {\n  data() {\n    return {\n      model: {\n        email: \"\",\n        password: \"\"\n      },\n      fields: [\n        {\n          type: \"input\",\n          modelKey: \"email\",\n          label: \"email\",\n          props: {\n            placeholder: \"Please Enter Email\"\n          },\n          rules: {\n            required: true\n          },\n          messages: {\n            required: \"Email Cannot Be Empty!\"\n          }\n        },\n        {\n          type: \"input\",\n          modelKey: \"password\",\n          label: \"password\",\n          props: {\n            placeholder: \"Please Enter Password\",\n            type: \"password\",\n            eye: {\n              open: false\n            }\n          },\n          rules: {\n            required: true\n          },\n          messages: {\n            required: \"Password Cannot Be Empty!!\"\n          }\n        }\n      ]\n    };\n  },\n  methods: {\n    submitHandler(e, model) {\n      e.preventDefault();\n      //调用注册接口\n      var info = {};\n      //console.log(model);\n      info.email = model.email;\n      info.password = model.password;\n      //console.log(info);\n      loginApi(info).then(\n        res => {\n\n          \n          if (res.code === 0) {\n            //登录成功，跳转到个人中心\n            localStorage.setItem('token',res.data)\n            this.$store.dispatch('set_Token',res.data)\n\n            //跳转页面, 根据业务需要\n            this.$router.push({path:'/personal'})\n\n          }else{\n             const toast = this.$createToast({\n              txt: \"Login Fail\",\n              type: \"error\",\n              time: 1500\n            });\n            toast.show();\n          }\n        }\n      );\n    },\n    goIndex:function() {\n      this.$router.push({path:'/index'})\n\n    },\n  }\n}\n  \n</script>\n<style lang=\"scss\" scoped>\n.main {\n  padding: 50px 5% 0;\n  text-align: center;\n}\n// 登录\n.cube-btn {\n  margin-top: 20px;\n}\n//注册\n.reg {\n  display: inline-block;\n  margin-top: 30px;\n  font-size: 18px;\n}\n.nav{\n  width: 100px;\n  height: 35px;\n  float: left;\n  margin-left: 30px;\n  margin-top: -110px;\n}\n.b1{\n    background-color: #ebf5fa;\n    color:black; \n    height: 35px;\n    width: 80px;\n    border: 0;\n    float: left;\n    margin-left: 30px;\n    text-align: center;\n    border-radius: 6px;\n    -webkit-transition-duration: 0.3s; /* Safari */\n    transition-duration: 0.3s;\n}\n.b1:hover{\n    background-color:#1b9ff7;\n    color: white;\n}\n\n.b3{\n    background-color: #1b9ff7;\n    color:white; \n    height: 35px;\n    border: 0;\n    width: 350px;\n    margin-top: 30px;\n    text-align: center;\n    font-size: 14pt;\n    border-radius: 6px;\n    -webkit-transition-duration: 0.3s; /* Safari */\n    transition-duration: 0.3s;\n}\n.b3:hover{\n    background-color: white;\n    color: #1b9ff7;\n}\n</style>"],"sourceRoot":"src/views/Login"}]}